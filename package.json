{
  "name": "cosmos-fundraiser-cli",
  "version": "2.0.2",
  "description": "CLI tool for purchasing Atoms in the Cosmos Fundraiser",
  "main": "build/main.js",
  "bin": {
    "cosmos-fundraiser": "./build/main.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf lib && babel --plugins transform-async-to-generator,transform-runtime --presets es2015 src -d lib && babel --plugins transform-async-to-generator,transform-runtime --presets es2015 bin/main.js > build/main.js",
    "source": "rm -rf lib && ln -s src lib",
    "prepublish": "npm run build",
    "publish": "npm run source"
  },
  "keywords": [
    "cosmos",
    "tendermint",
    "atoms",
    "bitcoin",
    "ethereum",
    "fundraiser",
    "sale"
  ],
  "author": "Matt Bell <mappum@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bitcoinjs-lib": "^2.3.0",
    "bluebird": "^3.5.0",
    "chalk": "^1.1.3",
    "cosmos-fundraiser": "^6.0.1",
    "inquirer": "^3.0.4",
    "ora": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0"
  }
}
